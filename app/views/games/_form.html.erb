<div>
  <%= f.label :description %>
  <%= f.text_field :description, value: @game.description %>
</div>

<div>
  <%= f.label :max_number_of_participants %>
  <%= f.number_field :max_number_of_participants, value: @game.max_number_of_participants %>
</div>

<div>
  <%= f.label :start_at %>
  <%= f.datetime_field :start_at, value: @game.start_at %>
</div>

<div>
  <%= f.label :end_at %>
  <%= f.datetime_field :end_at, value: @game.end_at %>
</div>

<div>
  <%= f.label :address %>
  <%= f.text_field :address, id: "autocomplete", placeholder: "Enter your address", value: @game.address %>
</div>

<div>
  <%= f.submit %>
</div>